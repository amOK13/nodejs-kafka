KAFKA_GROUP_ID=nodejs-kafka-group
KAFKA_TOPIC=test-topic

# Producer Configuration
KAFKA_PRODUCER_PRESET=balanced
KAFKA_PRODUCER_SOURCE=interactive-producer

# Producer Messages & Timeouts
KAFKA_DEFAULT_MESSAGE=Hello Enhanced Kafka!
